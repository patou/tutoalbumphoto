<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 1.6.4//EN" "http://google-web-toolkit.googlecode.com/svn/tags/1.6.4/distro-source/core/src/gwt-module.dtd">
<module rename-to='tutoalbumphoto'>
  <!-- Hérite du module principal de GWT                       -->
  <inherits name='com.google.gwt.user.User'/>

  <!-- On utile le style GWT Chrome, vous pouvez changer       -->
  <!-- le thème en décommantant la ligne du thème que          -->
  <!-- que vous voulez utiler                                  -->
  <!-- <inherits name='com.google.gwt.user.theme.standard.Standard'/>-->
  <inherits name='com.google.gwt.user.theme.chrome.Chrome'/>
  <!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/>  -->

  <!-- Spécifie la classe principale de GWT                    -->
  <!-- qui contient le onModuleLoad (équivalent du main)       -->
  <entry-point class='sfeir.gwt.albumphoto.client.Tutoalbumphoto'/>
</module>
